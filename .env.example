# Live trading env (example)
TG_TOKEN=YOUR_TELEGRAM_BOT_TOKEN
TG_CHAT=YOUR_CHAT_ID
TG_COMMANDS_ENABLE=1
REPORTS_ENABLE=1
REPORTS_SEND_ON_START=0
REPORTS_OUT_DIR=/tmp
REPORTS_STATE_PATH=/tmp/bybot_report_state.json

# Bybit REST base
BYBIT_BASE=https://api.bybit.com
TRADE_ACCOUNT_NAME=main

# Trade event DB (SQLite)
TRADE_DB_PATH=trades.db

# Enable live inplay module
ENABLE_INPLAY_TRADING=0
INPLAY_TOP_N=60
INPLAY_TRY_EVERY_SEC=30

# Enable retest levels module
ENABLE_RETEST_TRADING=0
RETEST_TOP_N=60
RETEST_TRY_EVERY_SEC=60
RETEST_TREND_ONLY=1
RETEST_LEVELS_ONLY_4H=1

# Accounts JSON (DO NOT commit real keys)
BYBIT_ACCOUNTS_JSON='[
  {
    "name": "main",
    "key": "YOUR_KEY",
    "secret": "YOUR_SECRET",
    "base": "https://api.bybit.com",
    "enabled": true,
    "position_mode": "oneway",
    "leverage": 2,
    "risk_pct": 0.01,
    "bot_capital_usd": 30,
    "cap_notional_to_equity": true,
    "reserve_equity_frac": 0.10,
    "max_positions": 2,
    "min_notional_usd": 6,
    "bounce_execute_trades": true,
    "bounce_top_n": 30
  }
]'

# Momentum continuation (backtest knobs)
MOMO_LOOKBACK_MIN=60
MOMO_MOVE_THRESHOLD_PCT=0.8
MOMO_PULLBACK_MAX_PCT=0.3
MOMO_EMA_PERIOD=20
MOMO_ATR_PERIOD=14
MOMO_SL_ATR_MULT=1.0
MOMO_RR=1.4
MOMO_COOLDOWN_BARS=12
MOMO_ALLOW_LONGS=1
MOMO_ALLOW_SHORTS=1
