## Breakout-only live (safe start)
## Use with: set -a; source configs/breakout_live_safe.env; set +a

ENABLE_INPLAY_TRADING=0
ENABLE_BREAKOUT_TRADING=1
ENABLE_RETEST_TRADING=0

BREAKOUT_TOP_N=16
BREAKOUT_TRY_EVERY_SEC=30

# Risk setup (0.5% to start; increase after 2-4 weeks)
# If you use BYBIT_ACCOUNTS_JSON, set risk_pct there to 0.005
RISK_PER_TRADE_PCT=0.5

# Symbol filters (denylist will be updated by /ban commands)
SYMBOL_FILTERS_PATH=/tmp/bybot_symbol_filters.json
SYMBOL_ALLOWLIST=ARCUSDT,RIVERUSDT,FARTCOINUSDT,LTCUSDT,SIRENUSDT,ZECUSDT,MOCAUSDT,SUIUSDT,WIFUSDT,1000PEPEUSDT,AVAXUSDT,ADAUSDT,DOGEUSDT,ARKUSDT,BCHUSDT,LAUSDT
SYMBOL_DENYLIST=

# Banlist recommendations (semi-auto)
RECO_ENABLE=1
RECO_SEND_ON_START=0
RECO_PERIOD_SEC=604800
RECO_LOOKBACK_DAYS=60
RECO_WORST_N=3
RECO_MIN_TRADES=8
RECO_STRATEGIES=inplay_breakout
